{
    "presets": [
        ["es2015", {
            "loose": true
        }],
        "react",
        "stage-3"
        // "@babel/env", 
        // "@babel/react"
    ],
    "plugins": [
        "dynamic-import-webpack",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css" // `style: true` 会加载 less 文件
        }],
        "react-html-attrs"
    ]
}